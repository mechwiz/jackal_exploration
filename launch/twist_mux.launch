<launch>
  <arg name="cmd_vel_out" default="cmd_vel/mux"/>

  <arg name="config_locks"  default="$(find jackal_exploration)/params/twist_mux_locks.yaml"/>
  <arg name="config_topics" default="$(find jackal_exploration)/params/twist_mux_topics.yaml"/>

  <arg name="config_joy" default="$(find jackal_exploration)/params/joystick.yaml"/>

  <node pkg="twist_mux" type="twist_mux" name="twist_mux" output="screen">
    <remap from="cmd_vel_out" to="$(arg cmd_vel_out)"/>

    <rosparam file="$(arg config_locks)"  command="load"/>
    <rosparam file="$(arg config_topics)" command="load"/>
  </node>

</launch>

